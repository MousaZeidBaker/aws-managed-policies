{
  "Document": {
    "Version": "2012-10-17",
    "Statement": [
      {
        "Effect": "Allow",
        "Action": [
          "s3tables:*"
        ],
        "Resource": "*"
      },
      {
        "Sid": "PassRoleToS3TablesReplication",
        "Effect": "Allow",
        "Action": [
          "iam:PassRole"
        ],
        "Resource": "*",
        "Condition": {
          "StringEquals": {
            "iam:PassedToService": [
              "replication.s3tables.amazonaws.com"
            ]
          }
        }
      }
    ]
  },
  "VersionId": "v2",
  "IsDefaultVersion": true,
  "CreateDate": "2025-11-22T01:04:08+00:00"
}
